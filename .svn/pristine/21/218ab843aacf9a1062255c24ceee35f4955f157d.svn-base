.normal {
  font-family: Georgia, sans-serif;
  text-align: center;
}

.title {
  font-size: 2.5rem;
  font-weight: normal;
  letter-spacing: -1px;
  background: darkslateblue;
  padding: .6em 0;
  color: white;
  margin: 0;
}

.trigger {
  height: 64px;
  margin: 0 10px;
  font-size: 22px;
  cursor: pointer;
  transition: all 0.3s, padding 0s;
}

.trigger:hover {
  color: #1890ff;
}

.logo {
  background:#fff;
}

.logo span {
  height:64px;
  display: block;
  float:left;
}


.ant-layout {
  height: 100%;
}


.topRightUser {
  margin: 0 20px;
  cursor: pointer;
  font-size: 16px;
}

.topRightUserHeadimg {
  margin-right: 7px;
  width: 36px;
  height: 36px;
}
